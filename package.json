{
  "homepage": "./",
  "name": "AGTReportAnalyzer",
  "main": "public/electron.js",
  "version": "1.0.1",
  "dependencies": {
    "@chakra-ui/icons": "^1.1.5",
    "@chakra-ui/react": "1.8.8",
    "@chakra-ui/system": "^1.12.1",
    "@chakra-ui/theme-tools": "^1.3.6",
    "@electron/remote": "^2.0.9",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@hypertheme-editor/chakra-ui": "^0.1.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@vitejs/plugin-react": "^3.1.0",
    "apexcharts": "^3.35.2",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chart.js": "^4.2.1",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.1.0",
    "faker": "^6.6.6",
    "framer-motion": "^4.1.17",
    "papaparse": "^5.3.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.0",
    "react-calendar": "^3.7.0",
    "react-chartjs-2": "^5.2.0",
    "react-custom-scrollbars-2": "^4.2.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^12.0.4",
    "react-icons": "^4.3.1",
    "react-is": "^18.0.0",
    "react-papaparse": "^4.1.0",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.0",
    "react-table": "^7.7.0",
    "serve": "^14.2.0",
    "stylis": "^4.1.1",
    "stylis-plugin-rtl": "2.0.2",
    "uplot": "^1.6.24",
    "uplot-react": "^1.1.4",
    "wait-on": "^7.0.1",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "sitemap": "babel-node ./sitemap-builder.js",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none npm start\" \"cross-env IS_DEV=true  electron --trace-warnings .\"",
    "electron:only": "cross-env IS_DEV=true electron .",
    "vite:elec": "cross-env BROWSER=none ELECTRON=true vite",
    "format": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none yarn start\" \"yarn electron:start\"",
    "electron:build": "yarn build && electron-builder -c.extraMetadata.main=build/electron.js",
    "electron:start": "wait-on tcp:3000 && electron ."
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "electron": "^23.1.1",
    "electron-react-devtools": "^0.5.3",
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^3.2.3",
    "prettier": "^2.8.4"
  },
  "author": "wklabs",
  "build": {
    "extends": null,
    "appId": "com.wklabs.is.reportAnalyzer",
    "productName": "reportAnalyzer",
    "copyright": "Copyright Â© 2023 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "oneClick": true,
      "allowToChangeInstallationDirectory": false,
      "shortcutName": "AGT Report Analyzer"
    },
    "win": {
      "target": "nsis"    },
    "files": [
      "build/**/*"
    ],
    "extraResources": [
      "preload.js"
    ],
    "directories": {
      "output": "dist"    }
  }
}
